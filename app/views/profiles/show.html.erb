<div class="profile-page-content">
  <h1><%= @user.name %>'s Profile</h1>
  <div class="profile-page-container">
    <p>Name: <%= @user.name %></p>
    <p>Email: <%= @user.email %></p>
      <li class="sign-out-button" style="margin-top: 20px;">
    <%= button_to(
      "Sign Out",
      destroy_user_session_path,
      method: :delete,
      class: "btn btn-danger",
      id: "sign-out-button"
    ) %>
    <br>
  </li>
</div>
